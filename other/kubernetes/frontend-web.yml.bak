apiVersion: apps/v1
kind: Deployment
metadata:
  name: epoweredmove-web-front-deployment
  labels:
    app: epoweredmove-web-front
spec:
  replicas: 1
  selector:
    matchLabels:
      app: epoweredmove-web-front
  template:
    metadata:
      labels:
        app: epoweredmove-web-front
    spec:
      containers:
      - name: epoweredmove-web-front
        image: diamantopoulosconstantinos/epoweredmove-web-front:latest
        ports:
        - containerPort: 80
---
apiVersion: v1
kind: Service
metadata:
  name: epoweredmove-web-front-service
spec:
  selector:
    app: epoweredmove-web-front
  type: ClusterIP
  ports:
    - protocol: TCP
      port: 8080
      targetPort: 80